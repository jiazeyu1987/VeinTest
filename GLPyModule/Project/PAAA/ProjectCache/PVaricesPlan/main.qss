QComboBox {
  background-color: #282828;
  border: 0px solid #525F7B;
  selection-background-color: #282828;
  font: 18px;
  padding-left: 12px; 
  color: #EEEEEE;
  border-radius: 4px;
}

QComboBox::drop-down {
  border: none;
  background: transparent;
  subcontrol-position: right;
  width: 40px;
}

QComboBox::drop-down:hover {
  
}

QComboBox::down-arrow {
  image: url(bin/images/arrow_down.png);
  padding-right: 8;
  padding-top: 1;
}
QComboBox::down-arrow:checked {
  image: url(bin/images/arrow_up.png);
  padding-right: 8;
  padding-top: 1;
}

QComboBox:!editable {
  background-color: #282828;
}

QTextBrowser {
  background-color: transparent;
  font: 14pt;
  color: #3377ff;
}

QAbstractSpinBox::up-button, QAbstractSpinBox::down-button {
  width: 16px;
}

QCheckBox, QCheckBox:hover, QCheckBox:disabled, QCheckBox:checked, QRadioButton, QRadioButton:hover, QRadioButton:disabled, QRadioButton:checked {
  background-color: none;
  font: 18px;
  spacing: 5px;
}

QCheckBox::indicator, QRadioButton::indicator {
  height: 26px;
  width: 26px;
  left: 2px;
}

QCheckBox::indicator:unchecked{
  image: url(bin/images/checkbox.png);
}

QCheckBox::indicator:checked{
  image: url(bin/images/checkbox1.png);
}

QRadioButton::indicator:unchecked {
  image: url(bin/images/radio.png);
}

QRadioButton::indicator:checked{
  image: url(bin/images/radio1.png);
}


QSlider {
  background-color: transparent;
}

QSlider::groove {
  background-color: #282828;
  border-radius: 0px;
}

QSlider::groove:hover {
  background-color: #282828;
}

QSlider::groove:horizontal {
  height: 4px;
}

QSlider::groove:vertical {
  width: 4px;
}

QSlider::handle {
  background-color: #65B7F3;
}

QSlider::handle:hover {
  /*background-color: #1c97ea;*/
}

QSlider::handle:pressed {
  background-color: #65B7F3;
}

QSlider::handle:horizontal {
  width: 8px;
  margin:-8px 0px -8px 0px;
}

QSlider::handle::vertical {
  margin: 0 -8px;
  height: 8px;
}

QSlider:sub-page{
  background-color: #65B7F3;
}
ctkRangeWidget QSlider{
    selection-background-color:#65B7F3;
}

QMenu {
  border: 1px solid #3e3e40;
}

/*QMenu QWidget {
  background-color: #de20b5;
}*/

QMenu::item {
  background-color: #434C5D;
  font: 12pt;
  min-height: 18px; /* 菜单项的最小高度 */
  min-width: 60px;
  padding: 5px 10px 5px 10px;/*上右下左*/
  /* margin: 2px 5px 2px 10px; 菜单项距其上下菜单项分别有2px，距菜单左右边界分别有10px和5px */
}

QMenu::item:selected {
  background-color: #3377ff;
}

QMenu::separator {
  height: 1px;
  background-color: #3e3e40;
}

QMenuBar {
	background-color: #025AD3;
  font: 18pt;
}

QMenuBar::item {
  font: 18pt;
  padding: 16 60px;
}

QMenuBar::item:selected {
  background-color: #3377ff;
}

QScrollBar {
  background-color: #1F1F1F;
  border-radius: 0px;
}

QScrollBar:horizontal {
  height: 10px;
  margin: 0 10px 0 10px;
}

QScrollBar:vertical {
  width: 10px;
  margin: 10px 0 10px 0;
}

QScrollBar::handle {
  background-color: #282828;
}

QScrollBar::handle:hover {
  background-color: #282828;
}

QScrollBar::handle:pressed {
  background-color: #282828;
}

QScrollBar::handle:horizontal {
  height: 10px;
  min-width: 40px;
  margin: 10px 0 10px 0;
}

QScrollBar::handle:vertical {
  width: 10px;
  min-height: 40px;
  margin: 0 10px 0 10px;
}

QScrollBar::add-page, QScrollBar::sub-page {
  background-color: #161616;
}

QScrollBar::add-line, QScrollBar::sub-line {
  background-color: #282828;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  width: 10px;
}

QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  width: 10px;
}

QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  height: 10px;
}

QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  height: 10px;
}

QScrollBar::add-line:horizontal:hover {
  background-color: #282828;
}

QScrollBar::sub-line:horizontal:hover {
  background-color: #282828;
}

QScrollBar::add-line:vertical:hover {
  background-color: #282828;
}

QScrollBar::sub-line:vertical:hover {
  background-color: #282828;
}

QScrollBar::up-arrow, QScrollBar:down-arrow {
  width: 10px;
  height: 8px;
}

QScrollBar:left-arrow, QScrollBar::right-arrow {
  width: 8px;
  height: 10px;
}

QScrollBar::down-arrow {
  image: url(bin/image/arrow_down.png);
}

QScrollBar::down-arrow:disabled {
  image: url(bin/image/arrow_down.png);
}

QScrollBar::down-arrow:hover {
  image: url(bin/image/arrow_down.png);
}

QScrollBar::down-arrow:pressed {
  image: url(bin/image/arrow_down.png);
}

QScrollBar::left-arrow {
  image: url(bin/image/arrow_left.png);
}

QScrollBar::left-arrow:disabled {
  image: url(bin/image/arrow_left.png);
}

QScrollBar::left-arrow:hover {
  image: url(bin/image/arrow_left.png);
}

QScrollBar::left-arrow:pressed {
  image: url(bin/image/arrow_left.png);
}

QScrollBar::right-arrow {
  image: url(bin/image/arrow_right.png);
}

QScrollBar::right-arrow:disabled {
  image: url(bin/image/arrow_right.png);
}

QScrollBar::right-arrow:hover {
  image: url(bin/image/arrow_right.png);
}

QScrollBar::right-arrow:pressed {
  image: url(bin/image/arrow_right.png);
}

QScrollBar::up-arrow {
  image: url(bin/image/arrow_up.png);
}

QScrollBar::up-arrow:disabled {
  image: url(bin/image/arrow_up.png);
}

QScrollBar::up-arrow:hover {
  image: url(bin/image/arrow_up.png);
}

QScrollBar::up-arrow:pressed {
  image: url(bin/image/arrow_up.png);
