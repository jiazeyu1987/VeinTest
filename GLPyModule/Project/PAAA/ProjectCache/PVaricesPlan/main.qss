QWidget {
  background-color: #1C1C1E;
  border: none;
  color: #EEEEEE;
}

QWidget:disabled {
  background-color: #1C1C1E;
  border-color: #434346;
  color: #656565;
}

QFrame{
  background-color:#1C1C1E;
}

QProgressBar {
    font:26px;
	border-radius:10px;
	text-align:center;
	border:1px solid #26324A;
	background-color:#1F2634;
}
QProgressBar:chunk{
	border-radius:10px;
	background-color:#3A4B6D;
}

QStackedWidget {
  background-color: transparent;
}
/*
QComboBox:hover, QLineEdit:hover, QAbstractSpinBox:hover {
  background-color: #22305b;
}*/

QAbstractButton:pressed, QAbstractButton:checked {
  background-color: #434346;
}
QToolButton,
QPushButton {
  font: 18px;
  background-color: #177DDC;
  border-radius: 4px;
  color:#EEEEEE;
}

QToolButton:hover,
QPushButton:hover {
  background-color: #1765AD;
}
QToolButton:pressed,
QPushButton:pressed {
  background-color: #1765AD;
}

QPushButton:checked, QToolButton:checked {
  background-color: #0D47A1; /* 比当前背景色更深的蓝色 */
  border: 2px solid #FFFFFF; /* 添加白色边框以突出显示 */
  color: #FFFFFF; /* 使文字颜色与背景形成对比 */
}


QPushButton:disabled {
  background-color: rgba(82,127,217, 51);
  color: #6977a3;
}

QLabel {
  color: #EEEEEE;
  background-color: transparent;
  font: 18px;
}

QLabel:disabled {
  color: #465686;
  background-color: transparent;
}

QTextEdit {
  color: #EEEEEE;
  background-color: #22305B;
  border: 0px;
}

QToolBar QToolButton {
  padding: 0px;
  background-color: #3377ff;
}

QToolButton#qt_toolbar_ext_button {
  background-color: transparent;
  border: none;
  min-width: 12px;
  max-width: 12px;
  padding: 0;
  qproperty-icon: url(bin/images/tool_btn_arrow.png);
}

QToolButton::menu-arrow {
  image: url(bin/images/tool_btn_arrow.png);
}

QToolBox::tab {
  border: 1px solid #3f3f46;
}

QToolBox::tab:hover {
  background-color: #434346;
}

QToolBox::tab:selected {
  background-color: #ff0000;
  border: 1px solid #1c97ea;
}

QAbstractItemView {
  alternate-background-color: #1b1b1c;
  background-color: transparent;
}

QAbstractItemView::item {
  background-color: transparent;
}

QAbstractItemView::item:selected {
  background-color: transparent;
  outline: 0px;
}

QHeaderView{
  background-color: #282828;
}

QHeaderView::section {
  background-color: #282828;
  border: 0px solid transparent;
  padding: 10px;
  font: 18px;
  color: #EEEEEE;
}

QHeaderView::section:horizontal {
  border-right: 1px solid #2B303B;
}

QHeaderView::section:vertical {
  border-bottom: 1px solid #2B303B;
}

QHeaderView::section:vertical:checked {
  background-color: #2B303B;
  background-image: url(bin/images/checkbox1.png);
  background-repeat: no-repeat;
  background-position: center center;
  padding: 8px;
}

QHeaderView::section:vertical:pressed {
  background-color: #2B303B;
  font-weight: bold;
}

QHeaderView::down-arrow {
  image: url(:/org.blueberry.ui.qt/dark/down-arrow.svg);
  height: 10px;
  width: 10px;
}

QHeaderView::down-arrow:hover {
  image: url(:/org.blueberry.ui.qt/dark/down-arrow-hover.svg);
}

QHeaderView::down-arrow:pressed {
  image: url(:/org.blueberry.ui.qt/dark/down-arrow-pressed.svg);
}

QHeaderView::up-arrow {
  image: url(:/org.blueberry.ui.qt/dark/up-arrow.svg);
  height: 10px;
  width: 10px;
}

QHeaderView::up-arrow:hover {
  image: url(:/org.blueberry.ui.qt/dark/up-arrow-hover.svg);
}

QHeaderView::up-arrow:pressed {
  image: url(:/org.blueberry.ui.qt/dark/up-arrow-pressed.svg);
}

QGroupBox, QGroupBox:disabled {
  border: 1px solid #152042;
  margin-top: 8px;
  padding-top: 8px;
  background-color: #1C1C1E;
}

QGroupBox::title {
  padding: 0 4px;
  subcontrol-origin: margin;
  subcontrol-position: top center;
}
