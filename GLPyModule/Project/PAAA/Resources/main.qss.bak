QWidget {
  background-color: #363D4A;
  border: none;
  color: #f1f1f1;
}

QWidget:disabled {
  background-color: #363D4A;
  border-color: #434346;
  color: #656565;
}

QWidget #tabFusion {
  background-color: #22305B;
}

QFrame{
  background-color:#121D32;
}

QProgressBar {
    font:26px;
	border-radius:16px;
	text-align:center;
	border:1px solid #26324A;
	background-color:#1F2634;
}
QProgressBar:chunk{
	border-radius:16px;
	background-color:#3A4B6D;
}

QStackedWidget {
  background-color: transparent;
}

QComboBox:hover, QLineEdit:hover, QAbstractSpinBox:hover {
  background-color: #22305b;
}

QAbstractButton:pressed, QAbstractButton:checked {
  background-color: #434346;
}
QToolButton,
QPushButton {
  border: 0px solid #3f3f46;
  padding: 1px 1px;
  font: 20px "Alibaba PuHuiTi 2.0 55 Regular";
  background-color: #525F7B;
  border-radius: 8px;
}

QToolButton:hover,
QPushButton:hover {
  background-color: #3377FF;
}
QToolButton:pressed,
QPushButton:pressed {
  background-color: #527FD9;
}

QPushButton:checked, QToolButton:checked {
  border: 0px solid #007acc;
  background-color: #527FD9;
}

QPushButton:disabled {
  background-color: rgba(82,127,217, 51);
  color: #6977a3;
}

QLabel {
  color: rgba(255, 255, 255, 240);
  background-color: transparent;
  font: 20px "Alibaba PuHuiTi 2.0 55 Regular";
}

QLabel:disabled {
  color: #465686;
  background-color: transparent;
}

QTextEdit {
  color: white;
  background-color: #22305B;
  border: 0px;
}

QToolBar QToolButton {
  padding: 0px;
  background-color: #3377ff;
}

QToolButton#qt_toolbar_ext_button {
  background-color: transparent;
  border: none;
  min-width: 12px;
  max-width: 12px;
  padding: 0;
  qproperty-icon: url(:/org.blueberry.ui.qt/dark/tight-right-arrow-textcolor.svg);
}

QToolBox::tab {
  border: 1px solid #3f3f46;
}

QToolBox::tab:hover {
  background-color: #434346;
}

QToolBox::tab:selected {
  background-color: #1c97ea;
  border: 1px solid #1c97ea;
}

QAbstractItemView {
  alternate-background-color: #1b1b1c;
  background-color: transparent;
}

QAbstractItemView::item {
  background-color: transparent;
}

QAbstractItemView::item:selected {
  background-color: transparent;
  outline: 0px;
}

QHeaderView{
  background-color: #22305b;
  font: 16pt "Alibaba PuHuiTi 2.0 55 Regular";
  color: white;
}

QHeaderView::section {
  background-color: #34415B;
  border: 1px solid transparent;
  padding: 0px;
}

QHeaderView::section:horizontal {
  border-right: 1px solid #2B303B;
}

QHeaderView::section:vertical {
  border-bottom: 1px solid #2B303B;
}

QHeaderView::section:vertical:checked {
  background-color: #1c97ea;
}

QHeaderView::section:vertical:pressed {
  background-color: #1c97ea;
