QGroupBox, QGroupBox:disabled {
  border: 1px solid #152042;
  margin-top: 8px;
  padding-top: 8px;
  background-color: #363D4A;
}

QGroupBox::title {
  padding: 0 4px;
  subcontrol-origin: margin;
  subcontrol-position: top center;
}

QLineEdit, QAbstractSpinBox {
  background-color: #1F1F1F;
  border: 1px solid #525F7B;
  font: 20px "Alibaba PuHuiTi 2.0 55 Regular";
  padding-left: 12px; 
  color: #ffffff;
  border-radius: 4px;
}

QComboBox {
  background-color: #1F1F1F;
  border: 1px solid #525F7B;
  selection-background-color: #3377ff;
  font: 20px "Alibaba PuHuiTi 2.0 55 Regular";
  padding-left: 12px; 
  color: #ffffff;
  border-radius: 4px;
}

QComboBox::drop-down {
  border: none;
  background: transparent;
  subcontrol-position: right;
  width: 40px;
}

QComboBox::drop-down:hover {
  
}

QComboBox::down-arrow {
  image: url(:/abc/blue/comboBoxDrowArrow.png);
  padding-right: 8;
