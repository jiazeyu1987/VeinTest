QMenu {
  border: 1px solid #3e3e40;
}

/*QMenu QWidget {
  background-color: #de20b5;
}*/

QMenu::item {
  background-color: #434C5D;
  font: 12pt "Alibaba PuHuiTi 2.0 55 Regular";
  min-height: 20px; /* 菜单项的最小高度 */
  min-width: 60px;
  padding: 5px 10px 5px 10px;/*上右下左*/
  /* margin: 2px 5px 2px 10px; 菜单项距其上下菜单项分别有2px，距菜单左右边界分别有10px和5px */
}

QMenu::item:selected {
  background-color: #3377ff;
}

QMenu::separator {
  height: 1px;
  background-color: #3e3e40;
}

QMenuBar {
	background-color: #025AD3;
  font: 18pt "Alibaba PuHuiTi 2.0 55 Regular";
}

QMenuBar::item {
  font: 18pt "Alibaba PuHuiTi 2.0 55 Regular";
  padding: 16 60px;
}

